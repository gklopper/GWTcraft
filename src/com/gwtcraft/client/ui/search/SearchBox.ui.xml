<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.characterSearch {
			height: 30px;
		}
		.underline {
			border-bottom-style: solid;
			border-bottom-width: 1px;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.underline}">
		<g:TextBox styleName="{style.characterSearch}" ui:field="search"/>
		<g:Button styleName="{style.characterSearch}" ui:field="button" >Search</g:Button>
	</g:FlowPanel>
</ui:UiBinder> 